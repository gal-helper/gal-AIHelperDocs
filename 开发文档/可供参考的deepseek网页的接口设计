// 用途：
用来获取用户所点击的当前会话的所有历史消息
// 示例：
https://chat.deepseek.com/api/v0/chat/history_messages?chat_session_id=134c1ca1-62fc-44f4-aa6f-56e8a8736955&cache_version=6

// 接口：
/chat_session/create    POST
// 参数：
{}
// 返回数据：
{
  "code": 0,
  "msg": "",
  "data": {
    "biz_code": 0,
    "biz_msg": "",
    "biz_data": {
      "id": "053699ee-7bdb-401b-89ea-db0e5453613f",
      "seq_id": 190429692,
      "agent": "chat",
      "title": null,
      "title_type": "WIP",
      "version": 0,
      "current_message_id": null,
      "pinned": false,
      "inserted_at": 1768266492.869,
      "updated_at": 1768266492.869
    }
  }
}
// 用途：
用来生成session_id，以及其他信息
// 示例：
https://chat.deepseek.com/api/v0/chat_session/create

// 接口：
/chat/create_pow_challenge
// 参数：
{
  "target_path": "/api/v0/chat/completion"
}
// 返回数据：
{
  "code": 0,
  "msg": "",
  "data": {
    "biz_code": 0,
    "biz_msg": "",
    "biz_data": {
      "challenge": {
        "algorithm": "DeepSeekHashV1",
        "challenge": "ee7726ed26c6c05cedba3dd5c78c73ce6f3b3e6b4853f1c35290de929677b765",
        "salt": "2de000a494d9213e785a",
        "signature": "10410630c0c41c79979ebe7487d57d8a048ea5dab28643bece62fa89f055f663",
        "difficulty": 144000,
        "expire_at": 1768266793001,
        "expire_after": 300000,
        "target_path": "/api/v0/chat/completion"
      }
    }
  }
}
